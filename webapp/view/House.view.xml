<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.untitledPrototype.controller.House" xmlns:uxap="sap.uxap"
	xmlns="sap.m">
	<uxap:ObjectPageLayout showTitleInHeaderContent="true" showHeaderContent="true" showAnchorBar="true" showFooter="false"
		subSectionLayout="TitleOnTop">
		<uxap:headerContent>
			<VBox alignItems="Stretch" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
				visible="true" displayInline="false">
				<items>
					<Link href="" text="{TYPE}" textAlign="Begin" width="auto" target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false"
						subtle="false"/>
					<Link text="{RENT}" textAlign="Begin" width="auto" target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false"
						subtle="false"/>
				</items>
				<layoutData/>
			</VBox>
			<Label text="AVAILABLE" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			<Link href="https://www.realestate.com.au/property-apartment-nsw-sydney-416741529" text="APPLY" textAlign="Begin" width="71px"
				target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false" subtle="false"/>
			<Button text="DIRECTIONS" type="Emphasized" iconFirst="true" width="280.00000000px" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress"/>
			<Button text="PHOTOS" type="Default" iconFirst="true" width="278px" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress1"/>
		</uxap:headerContent>
		<uxap:headerTitle>
			<uxap:ObjectPageHeader objectImageURI="/build/uilibraries/content/static/image-canvas.svg" objectTitle="{LOCATION}"
				objectSubtitle="{ACCOMODATIONTYPE}" headerDesign="Light" objectImageShape="Square" isObjectIconAlwaysVisible="false"
				isObjectTitleAlwaysVisible="false" isObjectSubtitleAlwaysVisible="false" isActionAreaAlwaysVisible="true">
				<uxap:actions>
					<Button text="BACK" type="Back" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress2"/>
				</uxap:actions>
				<uxap:breadCrumbsLinks/>
				<uxap:navigationBar/>
			</uxap:ObjectPageHeader>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection showTitle="true" title="" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="PARKING" mode="Expanded">
						<uxap:blocks>
							<Text text="{PARKING}" width="auto" maxLines="20" wrapping="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
						</uxap:blocks>
						<uxap:moreBlocks/>
						<uxap:actions/>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection showTitle="true" title="Page Section Title" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="INFO" mode="Expanded">
						<uxap:blocks>
							<Text text="{INFO}" width="auto" maxLines="20" wrapping="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
						</uxap:blocks>
						<uxap:moreBlocks/>
						<uxap:actions/>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection showTitle="true" title="Page Section Title" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Sub Section Title" mode="Expanded">
						<uxap:blocks/>
						<uxap:moreBlocks/>
						<uxap:actions/>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection showTitle="true" title="Page Section Title" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Sub Section Title" mode="Expanded">
						<uxap:blocks/>
						<uxap:moreBlocks/>
						<uxap:actions/>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
		</uxap:sections>
		<uxap:footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Approve" type="Accept" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<Button text="Reject" type="Reject" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<OverflowToolbarButton text="Action" type="Default" icon="sap-icon://action" iconFirst="true" width="auto" enabled="true" visible="true"
						iconDensityAware="false"/>
				</content>
			</OverflowToolbar>
		</uxap:footer>
	</uxap:ObjectPageLayout>
</mvc:View>